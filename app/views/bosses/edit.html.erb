<h1>Edit your Profile</h1>

<%= form_for @boss do |f| %>
  <%= f.label :name, "Full name" %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <br />

  <%= f.submit %>
<% end %>

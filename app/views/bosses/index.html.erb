<h3>All Bosses</h3>

<% @bosses.each do |boss| %>
<ul>
  <li><b>id: <%= boss.id %></b></li>
  <li><b>Name: </b><%= boss.name %></li>
  <li><b>Username: </b><%= boss.username %></li>
  <li><b>Email: </b><%= boss.email %></li>
  <li><b>City: </b><%= boss.city %></li>
  <li><b>Rating: </b><%= boss.average_rating %></li>
</ul>
<% end %>

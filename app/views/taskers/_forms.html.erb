<h1> Create a Tasker Profile</h1>

<%= form_for @tasker do |f| %>
  <%= f.label :name, "Full name" %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :bio %>
  <%= f.text_area :bio %><br>
  <%= f.label :vehicle %>
  <%= f.collection_selct :vehicle %>
  <%= f.submit %>
  <% end %>
